#include<bits/stdc++.h>
using namespace std;
int call_by_address(int &n){
	cout<<"address of n :"<<&n<<endl;
	n*=2;
	return n;	
}
int main(){
    int n=9;
	cout<<"address of n before function call:"<<&n<<endl;
	cout<<"value of n before function call:"<<n<<endl;
	cout<<"ans:"<<call_by_address(n)<<endl;
	cout<<"value of n after function call:"<<n;
	cout<<"address of n after function call:"<<&n<<endl;
}
